package aplicacao;

import java.util.Scanner;

public class Estrada<TIPO>{

	private int id;
	private Estrada<TIPO> inicio;
	private Estrada<TIPO> fim;
	
	public Estrada(int id, Estrada<TIPO> inicio, Estrada<TIPO> fim){
		this.id = id;
		this.inicio = inicio;
		this.fim = fim;
	}

	public Estrada<TIPO> getInicio() {
		return inicio;
	}

	public void setInicio(Estrada<TIPO> inicio) {
		this.inicio = inicio;
	}

	public Estrada<TIPO> getFim() {
		return fim;
	}

	public void setFim(Estrada<TIPO> fim) {
		this.fim = fim;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}
}
